{
  "name": "chula-util",
  "description": "Playground for testing/ playing around with typescript.",
  "version": "1.1.3",
  "main": "out/index.js",
  "types": "out/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DSCChula/chula-util.git"
  },
  "keywords": [
    "Chula"
  ],
  "author": "DSCChula",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DSCChula/chula-util/issues"
  },
  "homepage": "https://github.com/DSCChula/chula-util#readme",
  "dependencies": {
    "axios": "^0.20.0",
    "iconv-lite": "^0.6.2"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn src/index.ts",
    "build": "tsc",
    "test": "jest src",
    "test:watch": "jest --watch src",
    "format": "prettier src",
    "format:check": "prettier --check src",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "watch": "tsc --watch index.ts",
    "prepare": "yarn build",
    "preversion": "yarn lint",
    "prepublishOnly": "yarn test && yarn lint",
    "version": "yarn format && git add -A src"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.10.0",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.50",
    "typescript": "^3.9.5"
  }
}
